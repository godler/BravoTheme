{% set images =  page.media.images %}
{% set files =  page.media.files %}


<div class="container mx-auto">
	<div class="flex gap-5 items-center justify-center p-10">

		{% for image in images %}
			<div class="w-52">
				<img src="{{image.url}}"/>
			</div>
		{% endfor %}


		{% for file in files %}
			<div class="w-52">
				<img src="{{file.url}}"/>
			</div>
		{% endfor %}
	</div>
</div>
